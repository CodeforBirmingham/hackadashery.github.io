<body>
	<svg width="0" height="0" style="position: absolute; z-index: -1;">
		<defs>
			<pattern id="redstripe" patternUnits="userSpaceOnUse" width="20" height="20">
				<rect width="20" height="20" style="fill: rgba(255,0,0,0.2);"/>
				<path d="M-1,1 l2,-2            M0,20 l20,-20            M19,21 l2,-2" style="stroke:rgba(255,0,0,0.2);" stroke-width="2"/>
			</pattern>
		</defs>
	</svg>

	<div class="header js-header">
		<div class="header__sidebar">
			<h1 class="header__title">Philly 311</h1>
		</div>
		<div class="header__body">
			<div class="tabs">
				<div class="tabs__tab-col">
					<div class="tabs__tab js-nav-button" data-nav-href="search" data-nav-group="main">
						<div class="tabs__tab-text">Search</div>
					</div>
				</div>
				<div class="tabs__tab-col">
					<div class="tabs__tab js-nav-button" data-nav-href="explore" data-nav-group="main">
						<div class="tabs__tab-text">Explore</div>
					</div>
				</div>
				<div class="tabs__tab-col">
					<div class="tabs__tab js-nav-button" data-nav-href="landing" data-nav-group="main">
						<div class="tabs__tab-text">About</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="main js-main">

		<section id="landing" class="view js-nav-section landing" data-nav-group="main">
			{{> landing }}
		</section>

		<section id="search" class="view js-nav-section" data-nav-group="main">
			{{> map }}
		</section>

		<section id="submit" class="view js-nav-section" data-nav-group="main">
			{{> submit }}
		</section>

		<section id="explore" class="view js-nav-section" data-nav-group="main">
			{{> explore }}
		</section>


	</div>
</body>

